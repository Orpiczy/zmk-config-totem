#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/outputs.h>

// NUMBERS REPRESENT INDEXES OF LAYERS IN THE KEYMAP
#define BASE_WIN 0
#define BASE_MAC 2
#define OP_NUM_WIN 1 // OPERATIONS AND NUMBERS FOR WINDOWS
#define OP_NUM_MAC 3 // OPERATIONS AND NUMBERS FOR MAC
#define NAV_SYM 4 // NAVIGATION AND SYMBOLS
#define KB_CTRL 5 // KEYBOARD CONTROL


#define DUMMY_KEY F24 // DUMMY KEY

&mt {
  quick-tap-ms = <100>;
  global-quick-tap;
  flavor = "tap-preferred";
  tapping-term-ms = <200>;
};

/ {
     combos {
            compatible = "zmk,combos";
            combo_esc {
                timeout-ms = <50>;
                key-positions = <11 12 13>;
                bindings = <&kp ESC>;
            };
            combo_enter {
                timeout-ms = <50>;
                key-positions = <16 17 18>;
                bindings = <&kp ENTER>;
            };
            combo_control_layer {
                timeout-ms = <50>;
                key-positions = <20 31>;
                layers = <BASE_WIN BASE_MAC KB_CTRL>;
                bindings = <&tog KB_CTRL>;
            };
    };

    behaviors {
        td_quotes: tap_dance_0 {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <300>;
            bindings = <&kp SQT>, <&kp DQT>;
        };
        td_dots: tap_dance_1 {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp DOT>, <&kp COLON>;
        };
        td_sdots: tap_dance_2 {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp COMMA>, <&kp SEMICOLON>;
        };
        td_marks: tap_dance_3 {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <300>;
            bindings = <&kp QMARK>, <&kp EXCL>;
        };
        td_grave: tap_dance_4 {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp GRAVE>, <&kp TILDE>;
        };
        td_equal: tap_dance_5 {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp EQUAL>, <&kp LESS_THAN>, <&kp GREATER_THAN>;
        };
        td_line: tap_dance_6 {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp EQUAL>, <&kp UNDER>;
        };

        // NAV
        // td_win_layers: tap_dance_7 {
        //     compatible = "zmk,behavior-tap-dance";
        //     #binding-cells = <0>;
        //     tapping-term-ms = <200>;
        //     bindings = <&mo OP_NUM_WIN>, <&mo NAV_SYM>;
        // };
        // td_mac_layers: tap_dance_8 {
        //     compatible = "zmk,behavior-tap-dance";
        //     #binding-cells = <0>;
        //     tapping-term-ms = <200>;
        //     bindings = <&lt OP_NUM_WIN  DUMMY_KEY >, <&lt NAV_SYM DUMMY_KEY>;
        // };
    };

// WHAT DOES THIS MACRO DO?
    macros {
        gif: gif {
            label = "giphy";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LSHFT>
                , <&macro_tap &kp N2>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp G &kp I &kp F>
                ;
        };
    };

        keymap {
                compatible = "zmk,keymap";
